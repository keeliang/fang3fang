<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="fsf.beans.example.SystemUser" table="system_user">
		<id name="id" type="java.lang.Integer"  access="field">
			<column name="id" length="20" />
			<generator class="native" />
		</id>
		<property name="name" type="java.lang.String"  access="field" not-null="false">
			<column name="name" length="20" />
		</property>
		<property name="password" type="java.lang.String"  access="field" not-null="false">
			<column name="password" length="20" />
		</property>
	</class>
</hibernate-mapping>
