<struts>
	<!--******************* user *************** -->
	<package name="/sysadmin/sys/user" extends="baseDefault" namespace="/sysadmin/sys/user" >
		<action name="userList" class="userAction" method="doList">
			<result>/AdminPage/sys/user/userList.jsp</result>
			<result name="input">/AdminPage/sys/user/userList.jsp</result>
		</action>
		<action name="userSave" class="userAction" method="doSave">
			<result>/AdminPage/sys/user/userContent.jsp</result>
			<result name="input">/AdminPage/sys/user/userContent.jsp</result>
		</action>
		<action name="userEdit" class="userAction" method="doEdit">
			<result>/AdminPage/sys/user/userContent.jsp</result>
			<result name="input">/AdminPage/sys/user/userContent.jsp</result>
		</action>
		<action name="userNew" class="userAction" method="doNew">
			<result>/AdminPage/sys/user/userContent.jsp</result>
			<result name="input">/AdminPage/sys/user/userContent.jsp</result>
		</action>
		<action name="userDelete" class="userAction" method="doDelete">
			<result>/AdminPage/sys/user/userList.jsp</result>
			<result name="input">/AdminPage/sys/user/userList.jsp</result>
		</action>
	</package>
	<!-- ********************************************** -->
	
	
</struts>