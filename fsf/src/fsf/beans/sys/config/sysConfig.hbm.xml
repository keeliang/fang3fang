<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="fsf.beans.sys.config.SysConfig" table="sys_config">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="identity" />
		</id>
		<property name="item" column="item" type="java.lang.String" length="50"/>
		<property name="value" column="value" type="java.lang.String" length="200"/>
	</class>
</hibernate-mapping>