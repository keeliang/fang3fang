<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
	<diskStore path="java.io.tmpdir" />

	<defaultCache maxElementsInMemory="1000" eternal="false" overflowToDisk="true" 
		timeToIdleSeconds="120" timeToLiveSeconds="120" diskPersistent="false" 
		diskExpiryThreadIntervalSeconds="60" />
		
	<cache name="fsf.beans.sys.dict.DictItem" maxElementsInMemory="100" eternal="false" overflowToDisk="true" 
		timeToIdleSeconds="120" timeToLiveSeconds="120" 
		diskPersistent="false" diskExpiryThreadIntervalSeconds="60" />
		
	<cache name="fsf.beans.sys.province.Province" maxElementsInMemory="50" eternal="false" overflowToDisk="true" 
		timeToIdleSeconds="120" timeToLiveSeconds="120" 
		diskPersistent="false" diskExpiryThreadIntervalSeconds="60" />
		
	<cache name="fsf.beans.sys.city.City" maxElementsInMemory="500" eternal="false" overflowToDisk="true" 
		timeToIdleSeconds="120" timeToLiveSeconds="120" 
		diskPersistent="false" diskExpiryThreadIntervalSeconds="60" />
		
	<cache name="customer.DictItem" maxElementsInMemory="100" eternal="false" overflowToDisk="true" 
		timeToIdleSeconds="120" timeToLiveSeconds="120" 
		diskPersistent="false" diskExpiryThreadIntervalSeconds="60" />
		
</ehcache>
